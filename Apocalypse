#include <iostream>
#include <string>
#include <math.h>

int apocalypse(float y){
    double p = pow(2,y);
    std::string x = std::to_string(p);
    int n;
    if(x.find("666") != std::string::npos){
        std::cout << "Yes" << std::endl;
        n = 1;
    }
    else{
        std::cout << "No" << std::endl;
        n = 0;
    }
    return n;
}


//test uwu
int main(){
    int r = 157;
    int y = apocalypse(r);
    std::cout << y << std::endl;
    return 0;
}
