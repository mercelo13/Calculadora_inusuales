#include <iostream>
#include <string>
#include <math.h>

int apocalypse(int y){
    double p = pow(2,y);
    std::string x = std::to_string(p);
    int n;
    if(x.find("666") != std::string::npos){
        std::cout << "Yes" << std::endl;
        n = 1;
    }
    else{
        std::cout << "No" << std::endl;
        n = 0;
    }
    return n;
}
