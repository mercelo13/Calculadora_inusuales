#include <iostream>
#include <string.h>
#include <cmath>
#include <string>


int apocalypse(int x){
    std::cin >> x;
    int y = pow(2,x);
    std::string s = std::to_string(y);
    std::string num = "666";
    size_t find = s.find(num);
    if (find != std::string::npos){
        return true;
    }
    else{
        return false;
    }
    return 0;
}
