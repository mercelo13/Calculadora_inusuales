#include <iostream>
#include <cmath>
#include <string>
#include <vector>

bool is_prime(long n){
    if(n < 2) {
        return false;
    }
    for(long i = 2; i * i <= n; i++){
        if ((n % i) == 0){
            return false;
        }
    }
    return true;
}

int power(int num){
    std::vector<double> prime;
    for(int i = 0; i <= num; i++){
        if(is_prime(i) and (num % i == 0)){
            prime.push_back(i);
        }
    }
    int end;
    int ans = 1;
    int count;
    for(count = 0;count <= (prime.size()-1); count++){
        long hal = pow(prime[count],2);
        ans *= hal;
    }
    if(ans == num){
        end = 1;
    }
    else{
        end = 0;
    }
    return end;
}

//test

int main(){
    int p = power(36);
    std::cout << p << std::endl;
}
