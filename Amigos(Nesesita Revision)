#include <iostream>
#include <cmath>

bool is_prime(long n){
    if(n < 2) {
        return false;
    }
    for(long i = 2; i * i <= n; i++){
        if ((n % i) == 0){
            return false;
        }
    }
    return true;
}

int friendship(long a,long b){
    long sum_a = 0;
    long sum_b = 0;
    for(long i = 0;i <= a;i++){
        if(is_prime(i) and ((a % i) == 0)){
            sum_a += i;
        }
    }
    for(long j = 0;j <= b;j++){
        if(is_prime(j) and ((b % j) == 0)){
            sum_b += j;
        }
    }
    if((sum_b == a) and (sum_a == b)){
        return 1;
    }
    else{
        return 0;
    }
}

int main(){
    int n = friendship(284,220);
    std::cout << n << std::endl;
    return 0;
}
