#include <iostream>
#include <cmath>

int friendship(long a,long b){
    long sum_a = 0;
    long sum_b = 0;
    for(long i = 1;i < a;i++){
        if((a % i) == 0){
            sum_a += i;
        }
    }
    for(long j = 1;j < b;j++){
        if((b % j) == 0){
            sum_b += j;
        }
    }
    if((sum_b == a) and (sum_a == b)){
        return 1;
    }
    else{
        return 0;
    }
}

//Test UWU

int main(){
    int n = friendship(284,220);
    std::cout << n << std::endl;
    return 0;
}
