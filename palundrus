#include <iostream>
#include <cmath>
#include <cstring>

using namespace std;

void palindrom(){
    int a ;
    cout<<"escriba su numero: ";
    cin >>a;
    string reverse = to_string(a);
    int H = reverse.length();
    for (int i = 0; i<H ; i++ )
        swap (reverse[i];reverse[H-1-i]);
    cout<<reverse<<endl;
}
int main(){
    palindron();
    return 0;
}
